{
  "name": "calibre-web-app-backend",
  "version": "1.0.0",
  "description": "Backend for Calibre Web Interface",
  "main": "dist/index.js",
  "scripts": {
    "prestart": "npm install",
    "start": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "run": "node dist/index.js",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "5.2.4",
    "chokidar": "^4.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "nodemailer": "^7.0.3",
    "passport": "^0.7.0",
    "passport-oauth2": "^1.7.0",
    "sqlite3": "^5.1.6",
    "vite": "^6.3.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.1",
    "@types/node": "^22.15.17",
    "@types/nodemailer": "^6.4.14",
    "@types/passport": "^1.0.16",
    "@types/passport-oauth2": "^1.4.14",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^3.1.3",
    "supertest": "^7.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "vitest": "^3.1.3"
  }
}
